{
  "manifest_version": 3,
  "name": "Upwork Filters",
  "version": "1.0",
  "description": "Filter all $0 jobs in upwork feed",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Upwork Filters"
  }
}
